{
  "keymap": {
    "FMclear": ["C"],
    "FMcollapse": ["`"],
    "FMcopy": ["p", "P"],
    "FMcopyname": ["y", "yy"],
    "FMcut": ["x", "X"],
    "FMdelete": ["d", "D"],
    "FMhidden": ["."],
    "FMnew": ["a", "A"],
    "FMprimary": ["<enter>"],
    "FMrefresh": ["<c-r>"],
    "FMrename": ["r"],
    "FMsecondary": ["<tab>", "<2-leftmouse>"],
    "FMselect": ["c"]
  },
  "keys": {
    "FMopen": ["<leader>v"]
  },
  "name_ignore": [".git", ".DS_Store", "__pycache__"],
  "open_left": true,
  "path_ignore": [],
  "show_hidden": false,
  "use_icons": true,
  "width": 40
}
